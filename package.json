{
  "name": "homepage",
  "version": "0.0.1",
  "description": "Personal Homepage at www.lukee910.ch",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukee910/homepage.git"
  },
  "author": "Lukas Doerig <lukee910@gmail.com>",
  "bugs": {
    "url": "https://github.com/lukee910/homepage/issues"
  },
  "homepage": "http://www.lukee910.ch",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "build": "concurrently \"npm install\" \"npm run tsc @tsbuild.txt\"",
    "build:win": "npm run build && del /S \".dist\\*.map\"",
    "build:linux": "concurrently \"npm run build\" \"find ./.dist -name '*.map' -delete\""
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.17",
    "bootstrap": "^4.0.0-alpha.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.1",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.31",
    "tether": "^1.2.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^1.8.2",
    "typings": "^1.3.1"
  }
}
